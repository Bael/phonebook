<h1>Listing shops</h1>

<table>
  <tr>
    <th>Firstname</th>
    <th>Secondname</th>
    <th>Lastname</th>
    <th>Title</th>
    <th>Workphone</th>
    <th>Owncellphone</th>
    <th>Workcellphone</th>
    <th>Email</th>
    <th>Icq</th>
    <th>Description</th>
    <th>Reception</th>
    <th>Fax</th>
    <th>Name</th>
    <th>Contract</th>
    <th>Okpo</th>
    <th>Tgnl</th>
    <th>Scancontract</th>
    <th>Station</th>
    <th>Railway</th>
    <th>Postaddress</th>
    <th>Legaladdress</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shops.each do |shop| %>
  <tr>
    <td><%= shop.firstname %></td>
    <td><%= shop.secondname %></td>
    <td><%= shop.lastname %></td>
    <td><%= shop.title %></td>
    <td><%= shop.workphone %></td>
    <td><%= shop.owncellphone %></td>
    <td><%= shop.workcellphone %></td>
    <td><%= shop.email %></td>
    <td><%= shop.icq %></td>
    <td><%= shop.description %></td>
    <td><%= shop.reception %></td>
    <td><%= shop.fax %></td>
    <td><%= shop.name %></td>
    <td><%= shop.contract %></td>
    <td><%= shop.okpo %></td>
    <td><%= shop.tgnl %></td>
    <td><%= shop.scancontract %></td>
    <td><%= shop.station %></td>
    <td><%= shop.railway %></td>
    <td><%= shop.postaddress %></td>
    <td><%= shop.legaladdress %></td>
    <td><%= link_to 'Show', shop %></td>
    <td><%= link_to 'Edit', edit_shop_path(shop) %></td>
    <td><%= link_to 'Destroy', shop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

